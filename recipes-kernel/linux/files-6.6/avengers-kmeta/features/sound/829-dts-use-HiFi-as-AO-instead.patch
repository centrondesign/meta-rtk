 &hifi_rproc {
-    status = "disabled";
+    status = "okay";
 };
 
 &acpu_rproc {
-	firmware-name = "bluecore.audio.enc.A00-stark-audio-nonsecure";
+	firmware-name = "bluecore.audio.enc.A00-stark-noaudio-nonsecure-0112";
 	status = "okay";
 };
 
@@ -97,7 +97,7 @@ vcpu-rpmsg {
 
 	hifi-rpmsg {
 		rproc = <&hifi_rproc>;
-		status = "disabled";
+		status = "okay";
 	};
 
 	ve3-rpmsg {
