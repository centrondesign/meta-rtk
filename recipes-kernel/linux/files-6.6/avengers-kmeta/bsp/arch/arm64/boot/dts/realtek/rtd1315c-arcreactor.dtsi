/dts-v1/;

#include <dt-bindings/soc/realtek,pm.h>
#include <dt-bindings/thermal/thermal.h>
#include "rtd16xxb.dtsi"

/ {
	chosen {
		stdout-path = "serial0:460800n8";
		bootargs = "console=ttyS0,460800 8250.nr_uarts=3 init=/init loglevel=4 max_loop=64 loop.max_part=7";
	};

	aliases {
		serial0 = &uart0;
		serial1 = &uart1;
		serial2 = &uart2;
		rtc0 = &sw_rtc;
	};

	sw_rtc: sw-rtc {
		compatible = "realtek,emulated-rtc";
		realtek,ptime-reg = <&iso 0x64c>;
	};

	cpu_supp: pwm-regulator {
		compatible = "pwm-regulator";
		pinctrl-names = "default";
		pinctrl-0 = <&pwm2_0_pins>;
		pwms = <&pwm 2 8000>;
		status = "okay";

		regulator-name = "cpudvs";
		regulator-min-microvolt = <750000>;
		regulator-max-microvolt = <1200000>;
		regulator-boot-on;
		regulator-always-on;
		regulator-ramp-delay = <400>;

		voltage-table =
			<1200000 0>,
			<1187500 2>,
			<1175000 5>,
			<1162500 7>,
			<1150000 10>,
			<1137500 11>,
			<1125000 14>,
			<1112500 16>,
			<1100000 19>,
			<1087500 20>,
			<1075000 23>,
			<1062500 26>,
			<1050000 28>,
			<1037500 30>,
			<1025000 32>,
			<1012500 35>,
			<1000000 37>,
			<987500 40>,
			<975000 41>,
			<962500 44>,
			<950000 47>,
			<937500 49>,
			<925000 50>,
			<912500 53>,
			<900000 55>,
			<887500 58>,
			<875000 60>,
			<862500 62>,
			<850000 64>,
			<837500 67>,
			<825000 70>,
			<812500 71>,
			<800000 74>,
			<787500 76>,
			<775000 79>,
			<762500 80>,
			<750000 83>;
	};
};

&audio_out {
	pinctrl-names = "default";
	pinctrl-0 = <&spdif_pins>;
	status = "okay";
};

&cec {
	status = "okay";
};

&cp {
	status = "okay";
};

&cpu0 {
	cpu-supply = <&cpu_supp>;
	operating-points-v2 = <&cpu_opps>;
	#cooling-cells = <2>;
};

&cpu1 {
	cpu-supply = <&cpu_supp>;
	operating-points-v2 = <&cpu_opps>;
	#cooling-cells = <2>;
};

&cpu2 {
	cpu-supply = <&cpu_supp>;
	operating-points-v2 = <&cpu_opps>;
	#cooling-cells = <2>;
};

&cpu3 {
	cpu-supply = <&cpu_supp>;
	operating-points-v2 = <&cpu_opps>;
	#cooling-cells = <2>;
};

&cpu_dvfs {
	fss,opp-updated = <0>;
	fss,volt-correct = <25000>;
	fss,volt-step = <25000>;
	fss,volt-min = <775000>;
	fss,volt-max = <1087500>;

	bsv,opp-updated = <0>;
	bsv,volt-correct = <137500 150000>;
	bsv,volt-step = <25000>;
	bsv,volt-min = <800000>;
	bsv,volt-max = <1087500>;

	cpu_opps: opp-table-cpu {
		compatible = "operating-points-v2";
		opp-shared;
		opp1000: opp-1000000000 {
			opp-hz = /bits/ 64 <1000000000>;
			opp-microvolt = <800000>;
			opp-microvolt-fss = <0>;
			opp-microvolt-bsv = <0>;
			clock-latency-ns = <150000>;
			status = "okay";
		};

		opp1100: opp-1100000000 {
			opp-hz = /bits/ 64 <1100000000>;
			opp-microvolt = <825000>;
			opp-microvolt-fss = <0>;
			opp-microvolt-bsv = <0>;
			clock-latency-ns = <150000>;
			status = "okay";
		};

		opp1200: opp-1200000000 {
			opp-hz = /bits/ 64 <1200000000>;
			opp-microvolt = <850000>;
			opp-microvolt-fss = <0>;
			opp-microvolt-bsv = <0>;
			clock-latency-ns = <150000>;
			status = "okay";
		};

		opp1300: opp-1300000000 {
			opp-hz = /bits/ 64 <1300000000>;
			opp-microvolt = <875000>;
			opp-microvolt-fss = <0>;
			opp-microvolt-bsv = <0>;
			clock-latency-ns = <150000>;
			status = "okay";
		};

		opp1400: opp-1400000000 {
			opp-hz = /bits/ 64 <1400000000>;
			opp-microvolt = <900000>;
			opp-microvolt-fss = <0>;
			opp-microvolt-bsv = <0>;
			clock-latency-ns = <150000>;
			status = "okay";
		};

		opp1500: opp-1500000000 {
			opp-hz = /bits/ 64 <1500000000>;
			opp-microvolt = <937500>;
			opp-microvolt-fss = <0>;
			opp-microvolt-bsv = <0>;
			clock-latency-ns = <150000>;
			status = "okay";
			opp-suspend;
		};
	};
};

&cpu_thermal {
	trips {
		cpu_alert0: cpu-alert0 {
			temperature = <90000>;
			hysteresis = <0>;
			type = "passive";
		};

		cpu_alert1: cpu-alert1 {
			temperature = <105000>;
			hysteresis = <0>;
			type = "passive";
		};
	};

	cooling-maps {
		cpu-map0 {
			trip = <&cpu_alert0>;
			cooling-device = <&cpu0 THERMAL_NO_LIMIT 3>;
			contribution = <70>;
		};

		cpu-map1 {
			trip = <&cpu_alert1>;
			cooling-device = <&cpu0 4 THERMAL_NO_LIMIT>;
			contribution = <70>;
		};
	};
};

&cpu_tm {
	status = "okay";
};

&emmc {
	status = "okay";
};

&gpio {
	status = "okay";
};

&gpu {
	operating-points-v2 = <&gpu_opps>;
	status = "okay";

	gpu_opps: opp-table-gpu {
		compatible = "operating-points-v2",
			     "operating-points-v2-mali";

		opp-450000000 {
			opp-hz = /bits/ 64 <450000000>;
			opp-hz-real = /bits/ 64 <450000000>;
			opp-microvolt = <800000>;
			opp-suspend;
		};

		opp-550000000 {
			opp-hz = /bits/ 64 <550000000>;
			opp-hz-real = /bits/ 64 <550000000>;
			opp-microvolt = <800000>;
		};

		opp-650000000 {
			opp-hz = /bits/ 64 <650000000>;
			opp-hz-real = /bits/ 64 <650000000>;
			opp-microvolt = <800000>;
		};

		opp-702000000 {
			opp-hz = /bits/ 64 <702000000>;
			opp-hz-real = /bits/ 64 <702000000>;
			opp-microvolt = <800000>;
		};
	};
};

&hse {
	status = "okay";
};

&i2c_0 {
	status = "disabled";
};

&i2c_1 {
	status = "okay";
};

&i2c_3 {
	status = "disabled";
};

&i2c_4 {
	status = "disabled";
};

&i2c_5 {
	status = "disabled";
};

&irda {
	status = "okay";
};

&jpeg {
	status = "okay";
};

&nic {
	status = "disabled";
};

&pcie1 {
	status = "disabled";
};

&pcie1_phy {
	status = "disabled";
};

&pcie2 {
	status = "disabled";
};

&pcie2_phy {
	status = "disabled";
};

&pwm {
	status = "okay";

	pwm_2 {
		enable = <1>;
		clkout_div = <0x6b>;
		clksrc_div = <0x00>;
		duty_rate = <30>;
	};
};

&reboot_mode {
	status = "okay";
};

&rfkill {
	rfkill-gpios = <&gpio 61 GPIO_ACTIVE_HIGH>;
	status = "okay";
};

&lsadc {
	status = "okay";
};

&rng {
	status = "okay";
};

&rtk_fw_pm {
	status = "okay";
};

&rtk_pm {
	wakeup-flags = <STICK_NORMAL_MODE>;
	wakeup-gpio-list = <58 GPIO_WAKEUP_ENABLE GPIO_WAKEUP_ACTIVE_LOW>,
			   <60 GPIO_WAKEUP_ENABLE GPIO_WAKEUP_ACTIVE_LOW>;
	wakeup-timer = <0>;
	rtk-bt = <255>; /* for Realtek bluetooth only */
	status = "okay";
};

&sb2_inv {
	status = "okay";
};

&sd {
	status = "okay";
};

&sdio {
	wifi-rst-gpios = <&gpio 57 GPIO_ACTIVE_HIGH>;
	sd-power-gpios = <&gpio 30 GPIO_ACTIVE_HIGH>;
	pinctrl-names = "default",
			"sdio-3v3-drv",
			"sdio-1v8-drv",
			"sdio-vsel-3v3",
			"sdio-vsel-1v8";
	pinctrl-0 = <&sdio_pins_0>;
	pinctrl-1 = <&sdio_pins_0_3v3_drv>;
	pinctrl-2 = <&sdio_pins_0_1v8_drv>;
	pinctrl-3 = <&sdio_pins_0_vsel_3v3>;
	pinctrl-4 = <&sdio_pins_0_vsel_1v8>;
	bus-width = <4>;
	non-removable;
	cap-sd-highspeed;
	cap-mmc-highspeed;
	sd-uhs-sdr12;
	sd-uhs-sdr25;
	sd-uhs-sdr50;
	sd-uhs-sdr104;
	keep-power-in-suspend;
	enable-sdio-wakeup;
	no-sd;
	no-mmc;
	status = "okay";

	sd_card_slot {
		compatible = "sd-slot";
	};
};

&sound {
	status = "okay";
};

&tp {
	status = "okay";
};

&usb_port0_usb2phy {
	status = "okay";
};

&usb_port0 {
	realtek,power-gpio = <&gpio 48 GPIO_ACTIVE_LOW>;
	status = "okay";
};

&usb_port1_usb2phy {
	status = "disabled";
};

&usb_port1 {
	status = "disabled";
};

&usb_port2_usb2phy {
	status = "disabled";
};

&usb_port2_usb3phy {
	status = "disabled";
};

&usb_port2 {
	status = "disabled";

	dwc3@50000 {
		phys = <&usb_port2_usb2phy>;
		phy-names = "usb2-phy";
		maximum-speed = "high-speed";
	};
};

&type_c {
	realtek,usb-port = <&usb_port0>;
	status = "disabled";
};

/* debug console (J1) */
&uart0 {
	status = "okay";
};

/* M.2 slot (CON2) */
&uart1 {
	status = "okay";
};

&uart2 {
	status = "disabled";
};

&usb_manager {
	status = "okay";
};

&ve1 {
	status = "okay";
};

&watchdog {
	status = "okay";
};

&rtk_avcpu {
	status = "okay";
};
