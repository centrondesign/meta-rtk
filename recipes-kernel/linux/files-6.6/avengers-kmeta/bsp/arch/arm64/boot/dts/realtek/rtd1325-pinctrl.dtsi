&pinctrl {
	sd_pins: sd-pins {
		pins = "gpio_32", "gpio_33", "hif_data", "hif_en", "hif_rdy", "hif_clk";
		function = "sd";
	};

	sd_pins_vsel_3v3: sd-pins-vsel-3v3 {
		mux {
			pins = "gpio_32", "gpio_33", "hif_data", "hif_en", "hif_rdy", "hif_clk";
			function = "sd";
		};

		conf {
			pins = "gpio_32", "gpio_33", "hif_data", "hif_en", "hif_rdy", "hif_clk";
			power-source = <1>;
		};
	};

	sd_pins_vsel_1v8: sd-pins-vsel-1v8 {
		mux {
			pins = "gpio_32", "gpio_33", "hif_data", "hif_en", "hif_rdy", "hif_clk";
			function = "sd";
		};

		conf {
			pins = "gpio_32", "gpio_33", "hif_data", "hif_en", "hif_rdy", "hif_clk";
			power-source = <0>;
		};
	};

	sd_pins_3v3_drv: sd-pins-3v3-drv {
		mux {
			pins = "gpio_32", "gpio_33", "hif_data", "hif_en", "hif_rdy", "hif_clk";
			function = "sd";
		};

		conf {
			pins = "gpio_32", "gpio_33", "hif_data", "hif_en", "hif_rdy", "hif_clk";
			realtek,pdrive = <0>;
			realtek,ndrive = <0>;
