		mux {
			pins = "gpio_32", "gpio_33", "hif_data", "hif_en", "hif_rdy", "hif_clk";
			function = "sd";
		};

		conf-pull-disable {
			pins = "gpio_33";
			bias-disable;
		};

		conf-pull-up {
			pins = "hif_en";
			bias-pull-up = <1>;
		};
