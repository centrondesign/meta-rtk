				      <0x1b300 0x18>; /* SPI wrapper */
				pinctrl-names = "default";
				pinctrl-0 = <&gspi_pins_0>;
				num-chipselect = <1>;
				bus-num = <0>;
				clocks = <&cc RTD1619B_CRT_CLK_EN_GSPI>;
				clock-frequency = <256000000>;
				resets = <&cc RTD1619B_CRT_RSTN_GSPI>;
